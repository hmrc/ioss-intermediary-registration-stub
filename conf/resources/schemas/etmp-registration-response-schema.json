{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "processingDateTime",
    "formBundleNumber",
    "businessPartner"
  ],
  "properties": {
    "processingDateTime": {
      "type": "string",
      "pattern": "^[0-9]{4}-([0][1-9]|[1][0-2])-([0][1-9]|[1-2][0-9]|[3][0-1])T([0-1][0-9]|[2][0-3]):[0-5][0-9]:[0-5][0-9]Z$"
    },
    "formBundleNumber": {
      "description": "fb number",
      "type": "string",
      "minLength": 1,
      "maxLength": 12
    },
    "vrn": {
      "description": "vrn ",
      "type": "string",
      "minLength": 1,
      "maxLength": 9
    },
    "iossReference": {
      "description": "IOSS Reference ",
      "type": "string",
      "minLength": 1,
      "maxLength": 12
    },
    "intRef": {
      "description": "IOSS Intermediary Reference",
      "type": "string",
      "minLength": 1,
      "maxLength": 20
    },
    "businessPartner": {
      "description": "BP Number",
      "type": "string",
      "minLength": 1,
      "maxLength": 10
    }
  },

  "additionalProperties": false
}