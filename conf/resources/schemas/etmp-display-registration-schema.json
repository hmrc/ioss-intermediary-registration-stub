{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "customerIdentification": {
      "type": "object",
      "properties": {
        "idType": {
          "description": "idType",
          "type": "string",
          "minLength": 1,
          "maxLength": 6,
          "enum": [
            "VRN",
            "NINO",
            "UTR",
            "FTR"
          ]
        },
        "idValue": {
          "description": "idValue",
          "type": "string",
          "minLength": 1,
          "maxLength": 50
        }
      },
      "required": [
        "idType",
        "idValue"
      ],
      "additionalProperties": false
    },
    "tradingNames": {
      "description": "trading names",
      "type": "array",
      "maxItems": 10,
      "items": {
        "type": "object",
        "properties": {
          "tradingName": {
            "description": "trading name",
            "type": "string",
            "minLength": 1,
            "maxLength": 100
          }
        },
        "required": [
          "tradingName"
        ],
        "additionalProperties": false
      }
    },
    "clientDetails": {
      "type": "array",
      "maxItems": 99,
      "items": {
        "type": "object",
        "properties": {
          "clientName": {
            "type": "string",
            "minLength": 1,
            "maxLength": 200
          },
          "clientIossID": {
            "type": "string",
            "minLength": 1,
            "maxLength": 50
          },
          "clientExcluded": {
            "type": "boolean",
            "minLength": 1,
            "maxLength": 5
          }

        },
        "required": [
          "clientName",
          "clientIossID",
          "clientExcluded"
        ],
        "additionalProperties": false
      }
    },

    "intermediaryDetails": {
      "type": "object",
      "properties": {
        "otherIossIntermediaryRegistrations": {
          "type": "array",
          "maxItems": 99,
          "items": {
            "type": "object",
            "properties": {
              "issuedBy": {
                "type": "string",
                "minLength": 1,
                "maxLength": 2
              },
              "intermediaryNumber": {
                "type": "string",
                "minLength": 1,
                "maxLength": 25
              }
            },
            "required": [
              "intermediaryNumber",
              "issuedBy"
            ],
            "additionalProperties": false
          }
        }
      },
      "additionalProperties": false
    },
    "otherAddress": {
      "type": "object",
      "properties": {
        "issuedBy": {
          "type": "string",
          "minLength": 1,
          "maxLength": 2
        },
        "tradingName": {
          "type": "string",
          "minLength": 1,
          "maxLength": 40
        },
        "addressLine1": {
          "type": "string",
          "minLength": 1,
          "maxLength": 40
        },
        "addressLine2": {
          "type": "string",
          "minLength": 1,
          "maxLength": 40
        },
        "postcode": {
          "type": "string",
          "minLength": 1,
          "maxLength": 40
        },
        "regionOrState": {
          "type": "string",
          "minLength": 1,
          "maxLength": 40
        },
        "townOrCity": {
          "type": "string",
          "minLength": 1,
          "maxLength": 40
        }
      },
      "required": [
        "addressLine1",
        "issuedBy",
        "townOrCity"
      ],
      "additionalProperties": false
    },
    "schemeDetails": {
      "type": "object",
      "properties": {
        "commencementDate": {
          "description": "Commencement Date",
          "$ref": "#/definitions/date_type"
        },
        "requestDate": {
          "description": "Request Date",
          "$ref": "#/definitions/date_type"
        },
        "registrationDate": {
          "description": "Registration Date",
          "$ref": "#/definitions/date_type"
        },
        "nonCompliantReturns": {
          "description": "non compliant returns",
          "type": "string",
          "minLength": 1,
          "maxLength": 1,
          "enum": [
            "1",
            "2"
          ]
        },
        "nonCompliantPayments": {
          "description": "non compliant payments",
          "type": "string",
          "minLength": 1,
          "maxLength": 1,
          "enum": [
            "1",
            "2"
          ]
        },
        "registeredForTaxInEUCountries": {
          "description": "online market place - boolean type",
          "type": "boolean"
        },
        "euRegistrationDetails": {
          "description": "eu reg details",
          "type": "array",
          "maxItems": 99,
          "items": {
            "type": "object",
            "properties": {
              "issuedBy": {
                "description": "issued by - county ISO code",
                "type": "string",
                "minLength": 2,
                "maxLength": 2
              },
              "vatRegistered": {
                "description": "vat Registered - boolean type",
                "type": "boolean"
              },
              "vatNumber": {
                "description": "vat number",
                "type": "string",
                "minLength": 1,
                "maxLength": 12
              },
              "taxIdentificationNumber": {
                "description": "tax id number",
                "type": "string",
                "minLength": 1,
                "maxLength": 100
              },
              "fixedEstablishmentTradingName": {
                "description": "fixed est. trading name",
                "type": "string",
                "minLength": 1,
                "maxLength": 40
              },
              "fixedEstablishmentAddressLine1": {
                "description": "fixed Establishment Address Line1-Mandatory if Fixed Establishment checkbox is checked",
                "type": "string",
                "minLength": 1,
                "maxLength": 40
              },
              "fixedEstablishmentAddressLine2": {
                "description": "fixed Establishment Address Line2",
                "type": "string",
                "minLength": 1,
                "maxLength": 40
              },
              "townOrCity": {
                "description": "town or city- Mandatory if Fixed Establishment checkbox is checked",
                "type": "string",
                "minLength": 1,
                "maxLength": 40
              },
              "regionOrState": {
                "description": "region/state name",
                "type": "string",
                "minLength": 1,
                "maxLength": 40
              },
              "postcode": {
                "description": "postcode",
                "type": "string",
                "minLength": 1,
                "maxLength": 40
              }
            },
            "required": [
              "issuedBy",
              "fixedEstablishmentAddressLine1",
              "townOrCity"
            ],
            "additionalProperties": false
          }
        },
        "previouslyRegisteredfromOSSInEUCountries": {
          "description": "previously  Registered from OSS In EU Countries  - boolean type",
          "type": "boolean"
        },
        "previousEURegistrationDetails": {
          "description": "previous EU Registration details",
          "type": "array",
          "maxItems": 99,
          "items": {
            "type": "object",
            "properties": {
              "issuedBy": {
                "description": "issued by - county ISO code",
                "type": "string",
                "minLength": 2,
                "maxLength": 2
              },
              "registrationNumber": {
                "description": "registrationNumber",
                "type": "string",
                "minLength": 1,
                "maxLength": 12
              },
              "schemeType": {
                "description": "schemeType",
                "type": "string",
                "minLength": 1,
                "maxLength": 25,
                "enum": [
                  "OSS Union",
                  "OSS Non-Union",
                  "IOSS without intermediary",
                  "IOSS with intermediary"
                ]
              },
              "intermediaryNumber": {
                "description": "intermediaryNumber",
                "type": "string",
                "minLength": 1,
                "maxLength": 12
              }
            },
            "additionalProperties": false
          }
        },
        "websites": {
          "description": "websites",
          "type": "array",
          "minItems": 1,
          "maxItems": 99,
          "items": {
            "type": "object",
            "properties": {
              "websiteAddress": {
                "description": "website",
                "type": "string",
                "minLength": 1,
                "maxLength": 250
              }
            },
            "required": [
              "websiteAddress"
            ],
            "additionalProperties": false
          }
        },
        "contactDetails": {
          "type": "object",
          "properties": {
            "contactNameOrBusinessAddress": {
              "description": "contact name or bus. address",
              "type": "string",
              "minLength": 1,
              "maxLength": 100
            },
            "businessTelephoneNumber": {
              "description": "telephone number of business",
              "type": "string",
              "minLength": 1,
              "maxLength": 20
            },
            "businessEmailAddress": {
              "description": "email id of the business",
              "type": "string",
              "maxLength": 50,
              "format": "email"
            },
            "unusableStatus": {
              "description": "Unusable Status",
              "type": "boolean"
            }
          },
          "required": [
            "contactNameOrBusinessAddress",
            "businessTelephoneNumber",
            "businessEmailAddress",
            "unusableStatus"

          ]
        }
      },
      "required": [
        "commencementDate",
        "requestDate",
        "registrationDate",
        "registeredForTaxInEUCountries",
        "previouslyRegisteredfromOSSInEUCountries",
        "contactDetails"
      ],
      "additionalProperties": false
    },
    "exclusions": {
      "description": "exclusions",
      "type": "array",
      "maxItems": 99,
      "items": {
        "type": "object",
        "properties": {
          "exclusionReason": {
            "description": "exclusionReason",
            "type": "string",
            "minLength": 1,
            "maxLength": 2,
            "enum": [
              "-1",
              "1",
              "2",
              "3",
              "4",
              "5",
              "6"
            ]
          },
          "effectiveDate": {
            "description": "Effective Date",
            "$ref": "#/definitions/date_type"
          },
          "decisionDate": {
            "description": "Decision Date",
            "$ref": "#/definitions/date_type"
          },
          "quarantine": {
            "description": "quarantine - boolean type",
            "type": "boolean"
          }
        },
        "required": [
          "exclusionReason",
          "effectiveDate",
          "decisionDate",
          "quarantine"
        ],
        "additionalProperties": false
      }
    },
    "bankDetails": {
      "type": "object",
      "properties": {
        "accountName": {
          "type": "string",
          "minLength": 1,
          "maxLength": 70,
          "pattern": "^([A-Za-z0-9/\\-?:().,'+]+\\s)*[A-Za-z0-9/\\-?:().,'+]+$"
        },
        "bic": {
          "type": "string",
          "minLength": 8,
          "maxLength": 11,
          "pattern": "^[A-Z]{6}[A-Z0-9]{2}([A-Z0-9]{3})?$"
        },
        "iban": {
          "type": "string",
          "minLength": 14,
          "maxLength": 34,
          "pattern": "^[A-Z]{2}[0-9]{2}[0-9A-Z]{10,30}$"
        }
      },
      "required": [
        "accountName",
        "iban"
      ],
      "additionalProperties": false
    },
    "adminUse": {
      "type": "object",
      "properties": {
        "dataOrigin": {
          "type": "string",
          "minLength": 1,
          "maxLength": 10,
          "enum": [
            "Portal",
            "Online"
          ]
        },
        "completedOn": {
          "type": "string",
          "format": "date-time"
        },
        "completedBy": {
          "type": "string",
          "minLength": 1,
          "maxLength": 12
        },
        "dateOfApplication": {
          "description": "Date Of Application",
          "$ref": "#/definitions/date_type"
        },
        "changeDate": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "dataOrigin",
        "completedOn",
        "completedBy",
        "dateOfApplication",
        "changeDate"
      ],
      "additionalProperties": false
    }
  },
  "required": [
    "schemeDetails",
    "adminUse"
  ],
  "additionalProperties": false,
  "definitions": {
    "date_type": {
      "type": "string",
      "format": "date"
    }
  }
}